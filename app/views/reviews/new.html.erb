<div class="container text_center">
  <%= link_to "Back home", restaurants_path%>
  <h3>Add a review for:</h3>
  <h1><%=@restaurant.name%></h1>
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.button :submit  %>
  <% end %>
</div>
